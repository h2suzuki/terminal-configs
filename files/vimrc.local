let g:skip_defaults_vim = 1

set t_u7=
set vb t_vb=
set mouse=""
set background=dark
syntax on

set nohlsearch
set cursorcolumn
set cursorline

highlight CursorLine term=bold cterm=bold guibg=Grey40

set shiftwidth=4 tabstop=4 expandtab
set softtabstop=4

set list listchars=tab:>-,trail:@,precedes:<,extends:>
set ignorecase smartcase
